<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tolea.pw_lab3.MainActivity">

    <com.tolea.pw_lab3.DrawView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="10dp"
        />

    <com.tolea.pw_lab3.DrawView2
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="10dp"
        android:layout_alignParentRight="true"
        />

    <com.tolea.pw_lab3.DrawView3
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerInParent="true"
        />

    <com.tolea.pw_lab3.DrawView4
        android:layout_width="100dp"
        android:layout_marginTop="10dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        />

    <com.tolea.pw_lab3.DrawView5
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        />

    <com.tolea.pw_lab3.BezierDrawView1
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_centerVertical="true"
        />

    <com.tolea.pw_lab3.BezierDrawView2
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_centerVertical="true"
        android:layout_alignParentBottom="true"
        />

    <com.tolea.pw_lab3.CircleDrawView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginLeft="140dp"
        android:layout_marginTop="75dp"
        />

    <com.tolea.pw_lab3.SquareDrawView
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="250dp"
        />

    <com.tolea.pw_lab3.RectangleDrawView
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="325dp"
        />

    <com.tolea.pw_lab3.OvalDrawView
        android:layout_width="150dp"
        android:layout_height="125dp"
        android:layout_marginLeft="240dp"
        android:layout_marginTop="325dp"
        />

</RelativeLayout>
